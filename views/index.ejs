<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/css/styles.css">
    <title>Find A Git User</title>
  </head>
  <body>
    <div id="container">
      <header>
      </header>
      <nav>
      </nav>
      <div id="content">
        <h1>Blog with Rachel!</h1>
        <h4>Click on a title to view a blog.  Click "new post" to add your own!</h4><br>
        <% posts.forEach(function(posts) { %>
          <h3><a class="title" href="/<%=posts.id%>/post"><%= posts.title %></a></h3>
          <a href="/posts/<%=posts.id%>/edit" class="button">Edit</a><a href="/posts/<%=posts.id%>?method=delete" class="button">Delete</a><br><br>
        <% }); %>
        <br><br>
        <a href="/posts/new" class="button">New post</a>
      </div>
      <footer class="footer">
      </footer>
      <script src="./js/app.js"></script>
    </div>
  </body>
</html>
